<h1 class="text-center mb-3">{{title}}<h1>

<form action="/api/member" method="POST" class="mb-4">
    <div class="form-group">
        <label for="name"> Name</label>
        <input type="text" name="name" class="form-control">
    </div>

    <div class="form-group">
        <label for="email"> Email</label>
        <input type="email" name="email" class="form-control">
    </div>
    <input type="submit" value="add Member" class="btn btn-primary btn-block">

</form>

<h4>Members</h4>
<ul class="list-group">
    {{#each member}}
    <l1 class="list-group-item">{{this.name}}:{{this.email}}</l1>
    {{/each}}
</ul>

<a href="/api/member" class="btn btn-light mt-4">visit api</a>